{
  "name": "d3-hilbert",
  "version": "1.0.0",
  "description": "D3 layout to visualize distance variables using a continuous Hilbert space-filling space.",
  "keywords": [
    "d3",
    "hilbert"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "main": "dist/d3-hilbert.js",
  "module": "src/hilbert.js",
  "unpkg": "dist/d3-hilbert.min.js",
  "homepage": "https://github.com/vasturiano/d3-hilbert",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-hilbert.git"
  },
  "scripts": {
    "build": "rimraf dist && mkdir dist && rollup --banner \"$(preamble)\" -f umd -n d3 -o dist/d3-hilbert.js -- index.js",
    "minify": "uglifyjs dist/d3-hilbert.js -o dist/d3-hilbert.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {},
  "devDependencies": {
    "package-preamble": "0.1",
    "rimraf": "^2.6.2",
    "rollup": "0.63",
    "uglify-js": "3"
  }
}
